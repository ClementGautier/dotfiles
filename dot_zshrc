export ZSH="/home/clement/.oh-my-zsh"
export ZSH_THEME="powerlevel9k/powerlevel9k"
export MANPATH="/usr/local/man:$MANPATH"
export EDITOR="vim"
export ARCHFLAGS="-arch x86_64"
export SSH_KEY_PATH="~/.ssh/rsa_id"
export PATH=$HOME/bin:$HOME/.local/bin:/usr/local/bin:/snap/bin:$PATH
export PROJECT_DIR=$HOME/Projects/
export KUBECONFIG=$HOME/.kube/config

plugins=(
  git
  kubectl
  symfony2
)

source $ZSH/oh-my-zsh.sh
source <(stern --completion=zsh)
source /usr/share/google-cloud-sdk/completion.zsh.inc
source ~/.aliases

POWERLEVEL9K_LEFT_PROMPT_ELEMENTS=(kubecontext context dir vcs)

# added by travis gem
[ -f /home/clement/.travis/travis.sh ] && source /home/clement/.travis/travis.sh
